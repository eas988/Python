def double_index(lst, index):
  if index >= len(lst):
    return lst
  else:
    new_lst = lst[0:index]
    new_lst.append(lst[index]*2)
    new_lst = new_lst + lst[index+1:]
    return new_lst

def remove_middle(lst, start, end):
  first = lst[:start]
  last = lst[end+1:]
  new_lst = first + last
  return new_lst
